    "http-status": "^1.2.0",
    "ip": "^1.1.5",
    "joi": "^14.0.6",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "mongoose": "^5.3.8",
    "morgan": "~1.9.0",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.5",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
