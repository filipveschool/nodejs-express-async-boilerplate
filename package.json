{
  "name": "nodejs-express-async-boilerplate",
  "version": "0.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "private": true,
  "contributors": [],
  "homepage": "",
  "keywords": [
    "expressjs",
    "express",
    "boilerpate",
    "nodejs",
    "express",
    "async",
    "await",
    "es6",
    "es7",
    "rest",
    "api",
    "node",
    "mongodb",
    "passport"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "license": "MIT",
  "author": "Filip Vanden Eynde",
  "scripts": {
    "startdebug": "set DEBUG=nodejs-express-async-boilerplate:* & npm start",
    "startdebugreload": "set DEBUG=nodejs-express-async-boilerplate:* & nodemon start",
    "start": "cross-env NODE_ENV=development node app",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node app",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node app",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write",
    "test": "nyc mocha --timeout=10000 --exit"
  },
